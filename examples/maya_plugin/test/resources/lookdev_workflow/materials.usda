#usda 1.0
(
    defaultPrim = "mtl"
)

def Scope "mtl" (
    kind = "group"
)
{
    def Material "red"
    {
        token inputs:frame:stPrimvarName = "st"
        token outputs:surface.connect = </mtl/red/previewSurface.outputs:surface>

        def Shader "previewSurface"
        {
            uniform token info:id = "UsdPreviewSurface"
            float inputs:clearcoat = 0
            color3f inputs:diffuseColor = (1, 0, 0)
            color3f inputs:emissiveColor = (0, 0, 0)
            float inputs:ior = 1.52
            float inputs:metallic = 0
            float inputs:opacity = 1
            float inputs:roughness = 0.46
            bool inputs:useSpecularWorkflow
            token outputs:surface
        }

        def Shader "uvTexture"
        {
            uniform token info:id = "UsdUVTexture"
            asset inputs:file = @@
            float2 inputs:st.connect = </mtl/red/stReader.outputs:result>
            float3 outputs:rgb
        }

        def Shader "stReader"
        {
            uniform token info:id = "UsdPrimvarReader_float2"
            token inputs:varname.connect = </mtl/red.inputs:frame:stPrimvarName>
            float2 outputs:result
        }
    }

